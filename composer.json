{
    "name": "arnissolle/php-mfa",
    "description": "PHP implementation of Multi-factor authentication (MFA)",
    "keywords": [
        "php"
    ],
    "homepage": "https://github.com/arnissolle/php-mfa",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pierre Arnissolle",
            "email": "pierre@arnissolle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Arnissolle\\MFA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnissolle\\MFA\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
